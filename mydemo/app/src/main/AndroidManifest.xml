<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.mydemo" >


    <permission android:name="com.ylc.cly"></permission>
    <permission android:name="com.example.administrator.mydemo.permission.ylc"></permission>


    <uses-permission android:name="com.ylc.cly"></uses-permission>
    <uses-permission android:name="com.example.administrator.mydemo.permission.Myyyy"></uses-permission>


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity2"
            android:label="@string/app_name"
            android:permission="com.example.administrator.mydemo.permission.ylc">
            <intent-filter>
                <action android:name="com.example.administrator.mydemo.intent.action.MainActivity2"></action>
            </intent-filter>

        </activity>


    </application>

</manifest>
